<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>SilverStripe Modules by Firesphere</title>
    <parser>
        <target>all-api</target>
        <encoding>utf8</encoding>
        <extensions>
            <extension>php</extension>
        </extensions>
    </parser>
    <transformer>
        <target>all-api</target>
    </transformer>
    <transformations>
        <template name="responsive-twig" />
    </transformations>
    <files>
<!--        <directory>elastic</directory>-->
        <directory>search</directory>
        <directory>adblockwarning</directory>
        <directory>solr</directory>
        <directory>graphql-jwt</directory>
        <directory>cspheaders</directory>
        <directory>mailjet-mailer</directory>
        <directory>openstreetmaps</directory>
        <directory>healthcheck-jobs</directory>
        <ignore>**/tests/**/*</ignore>
        <ignore>solr/**/tests/**/*</ignore>
    </files>
</phpdoc>
